[
    {
      "selector": "node",
      "style": {
        "shape": "rectangle",
        "height": "14px",
        "width": "14px",
        "background-image": "data(img)",
        "background-fit": "cover",
        "background-color": "#000",
        "border-width": 0.5,
        "border-color": "#3d1f3d",
        "label": "data(label)",
        "font-size": 4,
        "text-justification": "center",
        "text-wrap": "wrap",
        "text-max-width": "38px",
        "text-margin-y": 2,
        "color": "#fff",
        "text-outline-color": "#000",
        "text-outline-width": 0.5,
        "text-valign": "bottom",
        "text-halign": "center"
      }
    },
    {
      "selector": ".undertale",
      "style": {
        "border-color": "#1E4E1E"
      }
    },
    {
      "selector": ".extra",
      "style": {
        "border-color": "#595959"
      }
    },
    {
      "selector": ".none",
      "style": {
        "opacity": "0.3"
      }
    },
    {
      "selector": ".hidden",
      "style": {
        "display": "none"
      }
    },
    {
      "selector": "edge",
      "style": {
        "width": 0.3,
        "line-color": "LightGrey",
        "opacity": 0.8,
        "curve-style": "unbundled-bezier",
        "control-point-step-size": 8,
        "target-arrow-color": "LightGrey",
        "target-arrow-shape": "triangle",
        "arrow-scale": 0.25,
        "target-distance-from-node": 2,
        "target-endpoint": "outside-to-line-or-label"
      }
    },
    {
      "selector": ".weak",
      "style": {
        "width": 0.4,
        "line-style": "dotted",
        "opacity": 0.6
      }
    },
    {

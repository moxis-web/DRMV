[
    {
        "selector": "node:selected",
        "style": {
            "border-style": "dotted"
        }
    },
    {
        "selector": "edge:selected",
        "style": {
            "line-outline-color": "white",
            "line-outline-width": 0.5
        }
    },
    {
        "selector": ".fade",
        "style": {
            "opacity": 0.2
        }
    },
    {
      "selector": "node",
      "style": {
        "shape": "rectangle",
        "height": "15px",
        "width": "15px",
        "background-image": "data(img)",
        "background-fit": "cover",
        "background-color": "#000",
        "outline-width": 0.7,
        "outline-color": "#3d1f3d",
        "label": "data(label)",
        "font-size": 3.5,
        "text-justification": "center",
        "text-wrap": "wrap",
        "text-max-width": "30px",
        "text-margin-y": 2,
        "color": "#fff",
        "text-outline-color": "#000",
        "text-outline-width": 0.4,
        "text-valign": "bottom",
        "text-halign": "center"
      }
    },
    {
      "selector": ".undertale",
      "style": {
        "outline-color": "#1E4E1E"
      }
    },
    {
      "selector": ".extra",
      "style": {
        "outline-color": "#595959"
      }
    },
    {
      "selector": ".none",
      "style": {
        "opacity": "0.3"
      }
    },
    {
      "selector": ".hidden",
      "style": {
        "display": "none"
      }
    },
    {
      "selector": "edge",
      "style": {
        "width": 0.25,
        "line-color": "LightGrey",
        "opacity": 0.8,
        "curve-style": "unbundled-bezier",
        "control-point-step-size": 8,
        "target-arrow-color": "LightGrey",
        "target-arrow-shape": "triangle",
        "arrow-scale": 0.25,
        "target-distance-from-node": 2,
        "target-endpoint": "outside-to-line-or-label"
      }
    },
    {
      "selector": ".strong",
      "style": {
        "width": 0.5,
        "opacity": 1
      }
    },
    {
      "selector": ".weak",
      "style": {
        "line-style": "dotted",
        "width": 0.35,
        "opacity": 0.6
      }
    },

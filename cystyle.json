[
    {
      "selector": "node",
      "style": {
        "shape": "rectangle",
        "height": "16px",
        "width": "16px",
        "background-image": "data(img)",
        "background-fit": "cover",
        "background-color": "#000",
        "border-width": 0.6,
        "border-color": "#3d1f3d",
        "label": "data(label)",
        "font-size": 4,
        "text-justification": "center",
        "text-wrap": "wrap",
        "text-max-width": "30px",
        "text-margin-y": 2,
        "color": "#fff",
        "text-outline-color": "#000",
        "text-outline-width": 0.5,
        "text-valign": "bottom",
        "text-halign": "center"
      }
    },
    {
      "selector": "[ost = 'Undertale']",
      "style": {
        "border-color": "#1E4E1E"
      }
    },
    {
      "selector": "[ost = 'Other']",
      "style": {
        "border-color": "#595959"
      }
    },
    {
      "selector": ".none",
      "style": {
        "opacity": "0.3"
      }
    },
    {
      "selector": ".hidden",
      "style": {
        "display": "none"
      }
    },
    {
      "selector": "edge",
      "style": {
        "width": 0.7,
        "line-color": "LightGrey",
        "opacity": 0.6,
        "curve-style": "unbundled-bezier",
        "control-point-step-size": 8,
        "target-arrow-color": "LightGrey",
        "target-arrow-shape": "triangle",
        "arrow-scale": 0.3,
        "target-distance-from-node": 2,
        "target-endpoint": "outside-to-line-or-label"
      }
    },
    {
      "selector": ".weak",
      "style": {
        "line-style": "dotted"
      }
    },

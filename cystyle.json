[
    {
      "selector": "node",
      "style": {
        "shape": "rectangle",
        "height": "15px",
        "width": "15px",
        "background-image": "data(img)",
        "background-fit": "cover",
        "background-color": "#000",
        "outline-width": 0.8,
        "outline-color": "purple",
        "label": "data(label)",
        "font-size": 3.5,
        "text-justification": "center",
        "text-wrap": "wrap",
        "text-max-width": "30px",
        "text-margin-y": 2,
        "color": "#fff",
        "text-outline-color": "#000",
        "text-outline-width": 0.4,
        "text-valign": "bottom",
        "text-halign": "center"
      }
    },
    {
        "selector": ":selected",
        "style": {
            "outline-color": "white",
            "outline-width": 1.4
        }
    },
    {
      "selector": ".undertale",
      "style": {
        "outline-color": "red"
      }
    },
    {
      "selector": ".other",
      "style": {
        "outline-color": "grey"
      }
    },
    {
      "selector": ".none",
      "style": {
        "opacity": "0.3"
      }
    },
    {
      "selector": "edge",
      "style": {
        "width": 0.4,
        "line-color": "LightGrey",
        "opacity": 0.9,
        "curve-style": "unbundled-bezier",
        "control-point-step-size": 8,
        "target-arrow-color": "LightGrey",
        "target-arrow-shape": "triangle",
        "arrow-scale": 0.25,
        "target-distance-from-node": 2,
        "target-endpoint": "outside-to-line-or-label"
      }
    },
    {
      "selector": ".strong",
      "style": {
        "width": 0.5,
        "opacity": 1
      }
    },
    {
      "selector": ".weak",
      "style": {
        "line-style": "dotted",
        "opacity": 0.6
      }
    },
    {
      "selector": "[motif = 'legend']",
      "style": {
        "line-color": "royalblue",
        "target-arrow-color": "royalblue"
      }
    },
    {
      "selector": "[motif = 'dont']",
      "style": {
        "line-color": "purple",
        "target-arrow-color": "purple"
      }
    }
  ]